<!DOCTYPE html>
<html >

  

<body>
<% include templates/header.ejs %>
	<% if(email) { %>
		<h2> <%= email %> </h2>
	<% } %>	
	<% if(email_registered) { %>
        <h2> <%= email_registered %> </h2>
    <% } %> 
    <% if(registration) { %>
        <h2> <%= registration %> </h2>
    <% } %>
  
	<h1>Register</h1>
    <form action="http://127.0.0.1:3000/myaction2" method="post">
    	<input type="text" name="n" placeholder="Name" required="required" > <br>
    	<input type="text" name="e" placeholder="Email" required="required" > <br>
    	<input type="text" name="c" placeholder="CGPA" required="required" > <br>
    	<input type="text" name="s" placeholder="Stream" required="required" > <br>
        <input type="password" name="p" placeholder="Password" required="required" > <br>
        <button type="submit">Register</button>
    </form>

  
    

</body>
</html>
