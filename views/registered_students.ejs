<!DOCTYPE html>
<html>
<head>
<style>
table {
    font-family: arial, sans-serif;
    border-collapse: collapse;
    width: 100%;
}

td, th {
    border: 1px solid #dddddd;
    text-align: left;
    padding: 8px;
}

tr:nth-child(even) {
    background-color: #dddddd;
}
</style>
</head>
<body>

<style>

ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
    background-color: #333;
}

li {
    float: left;
}

li a {
    display: block;
    color: white;
    text-align: center;
    padding: 14px 16px;
    text-decoration: none;
}

li a:hover {
    background-color: #111;
}
</style>

</head>
<body>

<ul>
  <li style="float:right"> <a href="http://127.0.0.1:3000/company_logout">Logout</a></li>
</ul>

<table>

  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>CGPA</th>
    <th>Stream</th>
  </tr>
  
  
<% for(var i=0; i<data.length; i++) {%>
<% var company_name = data[i].company %>

	<tr> 
	   <td><%= data[i].name %></td>
	   <td><%= data[i].email %></td>
	   <td><%= data[i].cgpa %></td>
       <td><%= data[i].stream %></td>
	</tr>
	
<% } %>
  

  
</table>

</body>
</html>
